{
    "name": "marella/phython",
    "description": "Call Python modules and functions from PHP.",
    "type": "library",
    "keywords": ["python", "library"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/marella/phython/issues",
        "source": "https://github.com/marella/phython"
    },
    "authors": [
        {
            "name": "Ravindra Marella",
            "email": "mv.ravindra007@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/process": "3.0.*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11",
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "4.8.*",
        "phpunit/php-code-coverage": "^2",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Phython\\": "src/Phython/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phython\\Tests\\": "tests/"
        }
    }
}
